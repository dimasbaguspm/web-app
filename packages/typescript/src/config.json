{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "target": "ES2021",                                
    "module": "ES2022",                               
    "moduleResolution": "bundler",
    "lib": ["DOM", "DOM.Iterable", "ES2021"],
    "jsx": "react-jsx",                      
    "types": [""],                           
    "outDir": "dist",           
    "esModuleInterop": true,                            
    "forceConsistentCasingInFileNames": true,           
    "strict": true,                                    
    "skipLibCheck": true                               
  }
}